@require "pod-styles"

#za-application {
  background: url("/assets/images/wallpaper-lebron.jpg") no-repeat center center fixed;
  -webkit-background-size: cover;
  -moz-background-size: cover;
  -o-background-size: cover;
  background-size: cover;
  -webkit-filter: blur(13px);
  -moz-filter: blur(13px);
  -o-filter: blur(13px);
  -ms-filter: blur(13px);
  filter: blur(13px);
  overflow-y: auto;
}

div.derp {
  position: absolute;
  top: 0;
  width: 100px;
  height: 50px;
  background: red;
  z-index: 3;
}

.table-max-height
  max-height: 400px;
  overflow:scroll;
  display: inline-block;

@require "./pods/*"
